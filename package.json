{
  "name": "7547-tp1-server",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./api/bin/www",
    "development": "npm install && nodemon -L ./api/bin/www",
    "pretest": "eslint --ignore-path .gitignore . --fix",
    "test": "mocha",
    "covered-test": "nyc mocha",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate --migrations-path api/config/sequelize/migrations --seeders-path api/config/sequelize/seeders --models-path api/models --config api/config/sequelize/config.json"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "morgan": "~1.9.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "request": "^2.88.0",
    "sequelize": "^5.1.0",
    "sequelize-cli": "^5.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codecov": "^3.2.0",
    "eslint": "^5.15.3",
    "eslint-config-strongloop": "^2.1.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  }
}
